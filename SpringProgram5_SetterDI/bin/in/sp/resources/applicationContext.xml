<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="in.sp.beans.Address" id="addrId">
	<property name="homeNo" value="201"/>
	<property name="address1" value="legal street"/>
	<property name="zipCode" value="12345"/>
	</bean>
	<bean class="in.sp.beans.Student" id="stdId">
		<property name="id" value="100" />
		<property name="name" value="Dharti N" />
		<property name="Address" ref="addrId" />

	</bean>

</beans>